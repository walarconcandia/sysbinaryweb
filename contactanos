    $(document).ready(function () {
        var $url = 'https://www.genotipo.com/';
        // CONDICIONES CHECKBOX
        $(".ind1").click(function () {
            $("#chk5").prop('checked', false);
            $(".chang").text("Platícanos un poco acerca de tu proyecto y necesidades: *");
        });

        $(".gnl").click(function () {
            $(".ind").prop('checked', false);
            $(".chang").text("Mensaje: *");
        });

        // FUNCIONES DE ENVIO //
        $("#enviar").click(function () {
            $("#enviar").fadeOut();
            $("#ajaxBusy").fadeIn(300);
            var pass = "1";
            var obj = {
                'accion': 'contacto',
                'forma': 'index'
            };

            $('.formulario input, .formulario textarea, .formulario select').each(function () {
                var data = $(this).val();
                if (data !== "") {
                    obj[$(this).attr('name')] = data;
                }
                $(this).removeClass('error');
                if (data === '' && (!$(this).hasClass('norequired'))) {
                    $(this).addClass('error');
                    pass = "0";
                    return false;
                }
            });

            var interes = "";
            $(".formulario input[type='checkbox']").each(function () {
                if ($(this).is(':checked')) {
                    interes = $(this).val() + ", " + interes;
                }
            });

            if (interes !== "") {
                obj["interes"] = interes;
            } else {
                $(".formulario input[type='checkbox']").addClass("error");
                pass = "0";
            }

            if (pass !== "0") {
                $.post('includes/_functions.php', obj, function (data) {
                    if (data === "0") {
                        $("#ajaxBusy").fadeOut(300);
                        $("#enviar").fadeIn();
                    } else {
                        window.location = $url+"gracias";
                    }
                });
            } else {
                $("#ajaxBusy").fadeOut(300);
                $("#enviar").fadeIn();
            }
        });

        // LOADER FULL SCREEN
        $('body').append('<div id="ajaxBusy"><div class="ui-widget-overlay"></div><img src="img/load.gif">      </div>');

        $('#ajaxBusy').css({
            display: "none",
            position: "fixed",
            top: "0px",
            left: "0px",
            height: $(window).height(),
            width: "100%",
            backgroundColor: "rgba(255,255,255, 0.9)",
            textAlign: "center",
            zIndex: "999"
        });

        $('#ajaxBusy img').css({
            paddingTop: "17%"
        });

    });
</script>

